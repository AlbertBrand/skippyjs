{
  "name": "skippyjs",
  "version": "0.1.6",
  "description": "SkippyJS, the faster test runner",
  "author": {
    "name": "Albert Brand",
    "email": "abrand@xebia.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlbertBrand/skippyjs.git"
  },
  "keywords": [
    "fast",
    "test runner",
    "test tool",
    "jasmine",
    "phantomjs",
    "multiprocess"
  ],
  "bin": {
    "skippyjs": "./bin/skippyjs"
  },
  "dependencies": {
    "babel": "5.8.*",
    "chokidar": "1.0.*",
    "colors": "1.1.*",
    "commander": "2.8.*",
    "di": "0.0.1",
    "es6-template-strings": "1.0.*",
    "fs-extra": "0.18.*",
    "glob-all": "3.0.*",
    "istanbul": "0.3.*",
    "lodash": "3.10.*",
    "lodash-contrib": "393.0.*",
    "mkdirp": "0.5.*",
    "phantom": "0.7.*",
    "phantomjs": "1.9.*",
    "promise-queue": "2.1.*",
    "st": "0.5.*"
  },
  "devDependencies": {
    "chai": "*",
    "mocha": "*"
  },
  "scripts": {
    "test": "babel-node -r node_modules/.bin/_mocha $npm_package_config_MOCHA_OPTS"
  },
  "config": {
    "MOCHA_OPTS": "test/ --recursive"
  }
}
